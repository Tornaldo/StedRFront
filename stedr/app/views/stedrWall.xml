<Alloy>
	<TabGroup>
		<Tab title="Wall">
			<Window id="welcomeWall" class="container" title="Velkommen">
				<Label id="wallTitle" text="{name}"></Label>
				<ScrollableView id="imageScroller" showPagingControl="true" ></ScrollableView>
			</Window>
		</Tab>
		<Tab title="Historier">
			<Window id="storyTab" class="container" title="Stories">
				<Label id="storyLabel"></Label>
			</Window>
		</Tab>
	</TabGroup>
</Alloy>