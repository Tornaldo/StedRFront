<Alloy>
	<TabGroup>
		<Tab title="Wall">
			<Window id="welcomeWall" class="container" title="{name}">
				<Label id="wallTitle"></Label>
				<ImageView id="wallImages"></ImageView>
				<Label id="wallDesc" text="{name}"></Label>
			</Window>
		</Tab>
		<Tab title="Stories">
			<Window id="storyTab" title="Stories">
				<Require type="widget" src="tiflexigrid" id="st"/>
				<Label id="storyLabel"></Label>
			</Window>
		</Tab>
	</TabGroup>
</Alloy>