<Alloy>
	<TableView dataCollection="tweets" dataTransform="transformFunction" class="twittertable">
		<TableViewRow>
			<ImageView id="twitterImage" image="{profileImage}" />
			<Label id="twitterName" text="{userName}" />
			<Label id="twitterText" text="{text}" />
		</TableViewRow>
	</TableView>
</Alloy>