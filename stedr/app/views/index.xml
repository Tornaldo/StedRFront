<Alloy>
	<Collection src="wall">
	<TabGroup>
		<Tab title="List">
			<Window class="container" title="Walls">
				<TableView dataCollection="wall">
					<TableViewRow class="tableView" title="{name}" />
				</TableView>
			</Window>
		</Tab>
		<Tab title="Map">
			<Window title="Wall" id="mapWin" onOpen="checkGooglePlayService">
			</Window>
		</Tab>	
	</TabGroup>
</Alloy>